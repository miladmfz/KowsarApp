<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".activity.find.Find_RegistrationActivity"
    android:orientation="vertical"
    android:layoutDirection="rtl">


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/find_register_a_appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:titleTextColor="?attr/colorOnPrimary"
            app:title="تفییر اطلاعات"
            >

        </androidx.appcompat.widget.Toolbar>


    </com.google.android.material.appbar.AppBarLayout>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="7dp">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="10dp">


                <Button
                    android:id="@+id/find_register_a_btn"
                    android:layout_width="match_parent"
                    android:text="@string/reg_save"

                    android:layout_weight="0.5"
                    app:backgroundTint="@color/green_600"
                    app:strokeColor="?attr/colorOnBackground"
                    app:strokeWidth="1dp"
                    android:layout_margin="3dp"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    style="@style/Widget.MaterialComponents.Button"
                    android:backgroundTint="?attr/colorPrimary"
                    android:textColor="?attr/colorOnPrimary"
                    />
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="8dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="اسم مجموعه:"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp"
                        android:paddingEnd="8dp" />

                    <!-- Value -->
                    <TextView
                        android:id="@+id/find_register_a_dbname"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_editbox"
                        android:padding="8dp"
                        android:gravity="center"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        tools:ignore="LabelFor" />
                </androidx.appcompat.widget.LinearLayoutCompat>


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="8dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Row of Buttons -->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <!-- Button: تنظیمات پیشفرض -->
                        <com.google.android.material.button.MaterialButton
                            android:id="@+id/find_register_a_replicationcolumn"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="3"
                            android:text="تنظیمات پیشفرض"
                            android:layout_margin="3dp"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            style="@style/Widget.MaterialComponents.Button"
                            android:backgroundTint="?attr/colorSecondary"
                            android:textColor="?attr/colorOnSecondary" />


                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="8dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Row 1: Config Broker -->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp">

                        <!-- Label -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|start"
                            android:text="@string/config_broker"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:paddingStart="8dp" />

                        <!-- EditText -->
                        <EditText
                            android:id="@+id/find_register_a_broker"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:gravity="center_vertical"
                            android:inputType="numberDecimal"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            tools:ignore="LabelFor" />
                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <!-- Row 2: Name Selector -->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <!-- Label -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|start"
                            android:text="نام بازاریاب"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:paddingStart="8dp" />

                        <!-- Spinner -->
                        <Spinner
                            android:id="@+id/find_register_a_spinnerbroker"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:gravity="center"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="8dp"
                    android:layout_margin="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp">

                        <!-- Label: Config Delay -->

                        <!-- Label: Config Item -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical|start"
                            android:text="آدرس سرور :"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:layout_weight="1"

                            android:paddingStart="8dp" />

                        <!-- Input: Config Grid -->
                        <EditText

                            android:id="@+id/find_register_a_serverurl"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:inputType="text"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:layout_weight="2"
                            android:textDirection="ltr"
                            android:layoutDirection="ltr"
                            android:gravity="center" />
                    </androidx.appcompat.widget.LinearLayoutCompat>


                    <!-- Row 1: Config Delay and Config Item -->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp">

                        <!-- Label: Config Delay -->

                        <!-- Label: Config Item -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical|start"
                            android:text="@string/config_item"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:layout_weight="1"

                            android:paddingStart="8dp" />

                        <!-- Input: Config Grid -->
                        <EditText

                            android:id="@+id/find_register_a_grid"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:inputType="numberDecimal"
                            android:maxLength="1"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:layout_weight="2"

                            android:gravity="center" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp">

                        <!-- Label: Config Delay -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical|start"
                            android:text="@string/config_delay"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:paddingStart="8dp"
                            android:layout_weight="1"
                            />

                        <!-- Input: Config Delay -->
                        <EditText
                            android:id="@+id/find_register_a_delay"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:inputType="text"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:gravity="center"
                            android:layout_weight="2"
                            />

                        <!-- Label: Config Item -->

                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <!-- Row 2: Title Size and Body Size -->
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp"
                        >

                        <!-- Label: Title Size -->

                        <!-- Label: Body Size -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|start"
                            android:text="سایز اطلاعات"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:paddingStart="8dp" />

                        <!-- Input: Body Size -->
                        <EditText
                            android:id="@+id/find_register_a_bodysize"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:inputType="text"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:gravity="center" />
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="8dp"
                        >

                        <!-- Label: Title Size -->
                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|start"
                            android:text="سایز عنوان ها"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:paddingStart="8dp" />

                        <!-- Input: Title Size -->
                        <EditText
                            android:id="@+id/find_register_a_titlesize"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/bg_editbox"
                            android:padding="8dp"
                            android:inputType="text"
                            android:textColor="?attr/colorOnBackground"
                            android:textSize="16sp"
                            android:gravity="center" />

                        <!-- Label: Body Size -->

                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>



                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp"
                    android:visibility="gone">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="@string/config_real_amount"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_real_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="false"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>



                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="انتخاب کلی متن تایپ شده بعد از جستجو "
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_selectallaftersearch"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="true"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>



                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="بسته شدن کیبورد"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_keyboardrunnable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="true"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="غیر فعال بودن تغییر توضیحات"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_editselected"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="true"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>




                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="نمایش تمام صفحه"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_showallpage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="true"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="8dp"
                    android:padding="8dp"
                    android:background="@drawable/bg_card"
                    android:elevation="4dp">

                    <!-- Label Text -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center_vertical|start"
                        android:text="قفل کردن قسمت جستجو"
                        android:textColor="?attr/colorOnBackground"
                        android:textSize="16sp"
                        android:paddingStart="8dp" />

                    <!-- Switch -->
                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/find_register_a_searchlock"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:enabled="true"
                        android:layout_gravity="center"
                        />
                </androidx.appcompat.widget.LinearLayoutCompat>





                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:padding="5dp"
                    android:visibility="gone">

                    <TextView
                        android:layout_weight="0.5"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="شماره تلفن همراه"
                        android:textColor="?attr/colorOnBackground"/>

                    <EditText
                        android:layout_weight="0.5"
                        android:id="@+id/find_register_a_phonenumber"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        android:inputType="text"
                        tools:ignore="LabelFor"
                        android:gravity="center"
                        android:textColor="?attr/colorOnBackground"/>


                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        app:cardBackgroundColor="?attr/colorSurface"
                        app:strokeColor="?attr/colorPrimary"
                        app:strokeWidth="2dp"
                        >
                        <LinearLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:padding="5dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Select Theme:"
                                android:layout_marginBottom="5dp"
                                android:textSize="18sp" />

                            <Spinner
                                android:id="@+id/find_themeSpinner"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:entries="@array/themes_array"
                                android:layout_marginBottom="5dp" />

                            <Button
                                android:id="@+id/find_applyButton"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Apply Theme"
                                android:layout_gravity="center_horizontal" />
                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        app:cardBackgroundColor="?attr/colorSurface"
                        app:strokeColor="?attr/colorPrimary"
                        app:strokeWidth="2dp"
                        >
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="10sp"
                            android:gravity="center_horizontal"
                            android:padding="16dp"
                            android:background="@drawable/bg_card"
                            >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_horizontal"
                                android:weightSum="2">

                                <!-- دکمه اصلی -->
                                <Button
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" Button"
                                    style="@style/Widget.MaterialComponents.Button"
                                    android:backgroundTint="?attr/colorPrimary"
                                    android:textColor="?attr/colorOnPrimary"
                                    android:layout_margin="3dp"
                                    android:layout_weight="1"/>

                                <!-- دکمه ثانویه -->
                                <Button
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=" Button"
                                    style="@style/Widget.MaterialComponents.Button"
                                    android:backgroundTint="?attr/colorSecondary"
                                    android:textColor="?attr/colorOnSecondary"
                                    android:layout_margin="3dp"

                                    android:layout_weight="1"/>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center_horizontal"
                                android:weightSum="2">


                                <!-- متن -->
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:layout_margin="3dp"
                                    android:text="Text"
                                    android:textColor="?attr/colorOnBackground"
                                    android:textAppearance="?attr/textAppearanceHeadline6"
                                    android:layout_weight="1"/>
                                <EditText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="5dp"
                                    android:hint="Enter text"
                                    android:gravity="center"

                                    android:background="@drawable/bg_editbox"
                                    android:textColor="?attr/colorOnBackground"
                                    android:padding="8dp"
                                    android:autofillHints=""
                                    tools:ignore="TextFields"
                                    android:layout_weight="1"/>
                            </LinearLayout>
                            <!-- فیلد متن -->

                        </LinearLayout>

                    </com.google.android.material.card.MaterialCardView>

                </androidx.appcompat.widget.LinearLayoutCompat>

            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.core.widget.NestedScrollView>
    </androidx.appcompat.widget.LinearLayoutCompat>



</androidx.appcompat.widget.LinearLayoutCompat>